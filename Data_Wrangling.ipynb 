   "outputs": [],
   "source": [
    "df['Reviews'].replace(r'.0M','000000',regex=True,inplace=True)\n",
    "df['Size'].replace(r'\\D','',regex=True,inplace=True)\n",
    "df['Installs'].replace(r'\\D','',regex=True,inplace=True)\n",
    "df['Price'].replace('[$]','',regex=True,inplace=True)"
   ]
